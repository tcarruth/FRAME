CSnams<-"Cod"




for(i in 1:length(CSnams)){
  
  outlist<-readRDS(paste0("CS/",CSnams[i],"/outlist.Rdata"))
  MPs<-readRDS(paste0("CS/",CSnams[i],"/MPs.Rdata"))
  
  jpeg(paste0("Figures/",CSnams[i],"_power.jpg"),res=500,width=9,height=11,units='in')  
    mahplot(outlist,MPs=MPs)
  dev.off()
  
}
