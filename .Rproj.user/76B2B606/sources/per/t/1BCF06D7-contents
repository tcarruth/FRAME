install.packages("MSEtool")
library(MSEtool)

#### MSE
SCA_MP <- make_MP(SCA, HCR40_10)
myMSE <- runMSE(DLMtool::testOM, MPs = c("FMSYref", "SCA_MP"))
plot(myMSE)

#### Assessment mode
myAssessment <- SCA(Data = ourReefFish)
plot(myAssessment)
retrospective(myAssessment)

## get TAC
SCA_MP(Data = ourReefFish)

# Alternatively:
HCR40_10(myAssessment)
