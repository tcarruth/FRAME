Thresh_tab<-function(Ptab1,thresh=c(70, 50, 70, 80, 50,  NA)){
Ptab2<-as.data.frame(matrix(c("NANANAN     ","NA   ",thresh),nrow=1))
names(Ptab2)<-c("MP","Type","1.1.1a","1.1.1b","1.1.2","1.2.1a","1.2.1b","LTY")
Ptab2 %>%
mutate(
#MP = row.names(.),
MP =   cell_spec(MP, "html", color = "white"),
MP_Type =   cell_spec(MP_Type, "html", color = "white"),
1.1.1a =   cell_spec(Crit_S, "html", color = "black"),
1.1.1b =   cell_spec(Caut_S, "html", color = "black"),
1.1.2 = cell_spec(Health_S, "html", color = "black"),
1.2.1a = cell_spec(OvFish_S, "html", color = "black"),
1.2.1b =  cell_spec(Yield_S, "html", color = "black"),
LTY =     cell_spec(Crit, "html", color = "black")
)%>%
#select(everything())%>%
knitr::kable("html", escape = F,align = "c") %>%
kable_styling("striped", full_width = F)%>%
column_spec(5, width = "3cm")  %>%
add_header_above(c("","","Thresholds" = 6))
}
